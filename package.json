{
  "name": "seo-website-audit",
  "version": "0.1.0",
  "description": "Simple command line and API server to perform SEO audits on a website",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "audit": "node dist/cmd/audit.js",
    "server": "node dist/cmd/server.js",
    "dev:audit": "node --no-warnings --loader ts-node/esm src/cmd/audit.ts",
    "dev:audit:watch": "nodemon --no-warnings --watch src --exec npm run dev:audit",
    "dev:server": "node --no-warnings --loader ts-node/esm src/cmd/server.ts",
    "dev:server:watch": "nodemon --no-warnings --watch src --exec npm run dev:server"
  },
  "keywords": [
    "seo",
    "audit",
    "command line",
    "api",
    "search engine optimization"
  ],
  "author": "Stanton Creative Media LLC.",
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "axios": "^1.12.2",
    "chrome-launcher": "^1.2.0",
    "crawlee": "^3.14.1",
    "dotenv": "^17.2.2",
    "lighthouse": "^12.8.2",
    "playwright": "^1.55.0",
    "prompts": "^2.4.2",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "@types/xml2js": "^0.4.14",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsc": "^2.0.4",
    "typescript": "^5.9.2"
  }
}
